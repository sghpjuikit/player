<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<fx:root type="javafx.scene.layout.StackPane" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <SplitPane dividerPositions="0.25">
      <items>
         <TreeView fx:id="groups" prefWidth="200.0" SplitPane.resizableWithParent="false" />
          <VBox>
              <HBox fx:id="controls" alignment="CENTER_LEFT" spacing="5.0">
                  <VBox.margin>
                    <Insets left="15.0" />
                  </VBox.margin>
              </HBox>
              <StackPane VBox.vgrow="ALWAYS">
                  <AnchorPane>
                      <StackPane.margin>
                          <Insets />
                      </StackPane.margin>
                      <padding>
                          <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                      </padding>
                      <ScrollPane fitToWidth="true" prefWidth="-1.0" vbarPolicy="AS_NEEDED" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                          <AnchorPane fx:id="configsRootPane" prefHeight="-1.0" prefWidth="-1.0" />
                      </ScrollPane>
                  </AnchorPane>
              </StackPane>
          </VBox>
      </items>
   </SplitPane>
</fx:root>
