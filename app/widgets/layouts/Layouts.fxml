<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root id="AnchorPane" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/8"
         xmlns:fx="http://javafx.com/fxml/1">
    <HBox spacing="5.0" AnchorPane.leftAnchor="240.0" AnchorPane.rightAnchor="15.0" AnchorPane.topAnchor="15.0">
        <Button mnemonicParsing="false" onMouseClicked="#newLayout" text="new" AnchorPane.rightAnchor="20.0"
                AnchorPane.topAnchor="20.0"/>
        <Button layoutX="451.0" layoutY="20.0" mnemonicParsing="false" onMouseClicked="#removeLayout" text="remove"/>
        <Button layoutX="383.0" layoutY="20.0" mnemonicParsing="false" onMouseClicked="#openLayoutDirectory"
                text="^ dir"/>
    </HBox>
    <VBox fx:id="box" alignment="CENTER" prefWidth="200.0" spacing="5.0" AnchorPane.bottomAnchor="15.0"
          AnchorPane.leftAnchor="15.0" AnchorPane.topAnchor="15.0">
        <ComboBox fx:id="layoutsCB" prefWidth="200.0" AnchorPane.leftAnchor="20.0" AnchorPane.rightAnchor="380.0"
                  AnchorPane.topAnchor="20.0"/>
        <HBox alignment="CENTER_LEFT" spacing="5.0">
            <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" prefWidth="48.0" text="Locked"
                   GridPane.columnIndex="0"
                   GridPane.rowIndex="1"/>
            <CheckBox fx:id="lockedChB" alignment="CENTER" mnemonicParsing="false" GridPane.columnIndex="1"
                      GridPane.rowIndex="1"/>
        </HBox>
        <HBox spacing="5.0">
            <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" prefWidth="48.0" text="Name"
                   GridPane.columnIndex="0"
                   GridPane.halignment="RIGHT" GridPane.rowIndex="0"/>
            <TextField fx:id="nameF" GridPane.columnIndex="1" GridPane.rowIndex="0"/>
        </HBox>
        <HBox alignment="CENTER" spacing="5.0">
            <Button id="loadB" mnemonicParsing="false" onMouseClicked="#saveSelectedLayout" prefHeight="20.0"
                    prefWidth="60.0" text="Save" AnchorPane.bottomAnchor="20.0" AnchorPane.leftAnchor="20.0"
                    AnchorPane.rightAnchor="20.0"/>
            <Button mnemonicParsing="false" onMouseClicked="#loadSelectedLayout" prefHeight="20.0" prefWidth="60.0"
                    text="Load" AnchorPane.bottomAnchor="45.0" AnchorPane.leftAnchor="20.0"
                    AnchorPane.rightAnchor="20.0"/>
        </HBox>
    </VBox>
    <StackPane fx:id="imgContainer" prefHeight="150.0" AnchorPane.leftAnchor="240.0" AnchorPane.rightAnchor="15.0"
               AnchorPane.topAnchor="50.0"/>
</fx:root>
