<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<fx:root type="javafx.scene.layout.StackPane" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <AnchorPane id="AnchorPane">
            <padding>
                <Insets left="5.0" right="5.0" />
            </padding>
            <HBox fx:id="layout" alignment="CENTER" spacing="15.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                <HBox fx:id="controlBox" alignment="CENTER" fillHeight="false" spacing="5.0" HBox.hgrow="NEVER">
                    <padding>
                      <Insets left="10.0" />
                    </padding>
                </HBox>
                <HBox alignment="CENTER" spacing="5.0" HBox.hgrow="NEVER">
                    <Label id="" fx:id="currTime" text="00:00" />
                </HBox>
            </HBox>
        </AnchorPane>
    </children>
</fx:root>