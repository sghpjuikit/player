<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Arc?>
<fx:root type="StackPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <StackPane fx:id="outer" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="15.0" prefWidth="15.0"
               StackPane.alignment="CENTER">
        <Arc fx:id="outer_arc" fill="#1f93ff00" length="270.0" radiusX="8.0" radiusY="8.0" strokeType="INSIDE"
             strokeWidth="2.0" styleClass="spinner" StackPane.alignment="TOP_LEFT"/>
    </StackPane>
    <StackPane fx:id="inner" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="10.0" prefWidth="10.0"
               StackPane.alignment="CENTER">
        <Arc fx:id="inner_arc" fill="#1f93ff00" length="270.0" radiusX="5.0" radiusY="5.0" startAngle="180.0"
             strokeType="INSIDE" strokeWidth="2.0" styleClass="spinner" StackPane.alignment="BOTTOM_RIGHT"/>
    </StackPane>
</fx:root>
