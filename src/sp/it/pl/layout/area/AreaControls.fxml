<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.TilePane?>
<fx:root id="AnchorPane" fx:id="root" pickOnBounds="false" type="AnchorPane" xmlns="http://javafx.com/javafx/8"
         xmlns:fx="http://javafx.com/fxml/1">
    <BorderPane fx:id="header" mouseTransparent="true" pickOnBounds="false"
                AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <center>
            <Label fx:id="title" text="Widget Name" BorderPane.alignment="CENTER"/>
        </center>
    </BorderPane>
    <Region fx:id="deactivator2" prefHeight="35.0" prefWidth="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
    <StackPane AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <Region fx:id="deactivator" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0"/>
        <TilePane fx:id="header_buttons" hgap="5.0" nodeOrientation="RIGHT_TO_LEFT" prefColumns="10" prefRows="1"
                  vgap="5.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
    </StackPane>
</fx:root>
