<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Accordion?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<fx:root maxWidth="-1.0" minHeight="-1.0" minWidth="-1.0" prefHeight="400.0" prefWidth="550.0"
         type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/8.0.40"
         xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
    </padding>
    <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
          AnchorPane.topAnchor="0.0">
        <HBox fx:id="controls" alignment="CENTER_LEFT" spacing="5.0">
            <VBox.margin>
              <Insets/>
            </VBox.margin>
        </HBox>
        <StackPane VBox.vgrow="ALWAYS">
            <AnchorPane>
                <StackPane.margin>
                    <Insets/>
                </StackPane.margin>
                <padding>
                    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                </padding>
                <ScrollPane fitToWidth="true" hbarPolicy="NEVER" prefWidth="-1.0" vbarPolicy="AS_NEEDED"
                            AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                            AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <AnchorPane minWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0">
                        <Accordion fx:id="accordion" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                   AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                    </AnchorPane>
                </ScrollPane>
            </AnchorPane>
        </StackPane>
    </VBox>
</fx:root>
