<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.Text?>
<AnchorPane id="AnchorPane" fx:id="root" prefHeight="580.0" prefWidth="858.0" xmlns="http://javafx.com/javafx/8"
            xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="30.0" left="30.0" right="30.0" top="30.0"/>
    </padding>
    <TitledPane collapsible="false" text="Games" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0"
                AnchorPane.topAnchor="0.0">
        <AnchorPane minHeight="0.0" minWidth="0.0">
            <ListView fx:id="game_list" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                      AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
        </AnchorPane>
    </TitledPane>
    <StackPane AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="350.0" AnchorPane.rightAnchor="250.0"
               AnchorPane.topAnchor="50.0">
        <StackPane fx:id="file_tree_root">
            <TreeView fx:id="file_tree" showRoot="false">
                <StackPane.margin>
                    <Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
                </StackPane.margin>
            </TreeView>
        </StackPane>
        <StackPane fx:id="cover_root">
            <StackPane.margin>
                <Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
            </StackPane.margin>
        </StackPane>
        <Path fill="#1f93ff00" styleClass="path-border" StackPane.alignment="TOP_LEFT">
            <elements>
                <MoveTo y="20.0"/>
                <LineTo/>
                <LineTo x="20.0"/>
            </elements>
        </Path>
        <Path fill="#1f93ff00" styleClass="path-border" StackPane.alignment="TOP_RIGHT">
            <elements>
                <MoveTo x="-20.0"/>
                <LineTo/>
                <LineTo y="20.0"/>
            </elements>
        </Path>
        <Path fill="#1f93ff00" styleClass="path-border" StackPane.alignment="BOTTOM_RIGHT">
            <elements>
                <MoveTo y="-20.0"/>
                <LineTo/>
                <LineTo x="-20.0"/>
            </elements>
        </Path>
        <Path fill="#1f93ff00" styleClass="path-border" StackPane.alignment="BOTTOM_LEFT">
            <elements>
                <MoveTo y="-20.0"/>
                <LineTo/>
                <LineTo x="20.0"/>
            </elements>
        </Path>
    </StackPane>
    <StackPane fx:id="inforootroot" layoutX="50.0" layoutY="50.0" maxHeight="200.0" maxWidth="300.0"
               minHeight="200.0" minWidth="300.0" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="0.0"
               AnchorPane.topAnchor="50.0">
        <StackPane fx:id="inforoot" StackPane.alignment="CENTER">
            <ScrollPane fitToHeight="true" fitToWidth="true" hbarPolicy="NEVER" StackPane.alignment="CENTER">
                <StackPane.margin>
                    <Insets bottom="15.0" left="15.0" right="15.0" top="15.0"/>
                </StackPane.margin>
                <AnchorPane>
                    <Text fx:id="info_text" strokeType="OUTSIDE" strokeWidth="0.0" styleClass="text-shape"
                          AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                          AnchorPane.topAnchor="0.0"/>
                </AnchorPane>
            </ScrollPane>
            <Path fill="#1f93ff00" styleClass="path-border" StackPane.alignment="TOP_LEFT">
                <elements>
                    <MoveTo y="20.0"/>
                    <LineTo/>
                    <LineTo x="20.0"/>
                </elements>
            </Path>
            <Path fill="#1f93ff00" styleClass="path-border" StackPane.alignment="TOP_RIGHT">
                <elements>
                    <MoveTo x="-20.0"/>
                    <LineTo/>
                    <LineTo y="20.0"/>
                </elements>
            </Path>
            <Path fill="#1f93ff00" styleClass="path-border" StackPane.alignment="BOTTOM_RIGHT">
                <elements>
                    <MoveTo y="-20.0"/>
                    <LineTo/>
                    <LineTo x="-20.0"/>
                </elements>
            </Path>
            <Path fill="#1f93ff00" styleClass="path-border" StackPane.alignment="BOTTOM_LEFT">
                <elements>
                    <MoveTo y="-20.0"/>
                    <LineTo/>
                    <LineTo x="20.0"/>
                </elements>
            </Path>
        </StackPane>
    </StackPane>
    <HBox fx:id="controls" alignment="CENTER" layoutX="95.0" layoutY="273.0" prefHeight="50.0"
          AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="250.0"/>
    <StackPane layoutX="150.0" layoutY="311.0" prefHeight="25.0" AnchorPane.bottomAnchor="-25.0"
               AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <Label fx:id="infoL"/>
    </StackPane>
    <Label fx:id="titleL" layoutX="50.0" layoutY="14.0" styleClass="game-title" AnchorPane.leftAnchor="0.0"
           AnchorPane.topAnchor="0.0"/>
</AnchorPane>
